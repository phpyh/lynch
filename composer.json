{
    "name": "phpyh/lynch",
    "description": "Phpyh Lynch",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov.valentin@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "vjik/scaffolder": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "symfony/var-dumper": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "PHPyh\\Lynch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPyh\\Lynch\\": "tests/"
        }
    },
    "config": {
        "bump-after-update": true,
        "sort-packages": true
    }
}
